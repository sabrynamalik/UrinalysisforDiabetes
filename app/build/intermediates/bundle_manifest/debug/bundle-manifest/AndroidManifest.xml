        <activity android:name="com.sabry.log.LogHome" />
        <activity android:name="com.sabry.log.homescreen" />
        <activity android:name="com.sabry.log.Results" />
        <activity android:name="com.sabry.log.EmailActivity" />
        <activity android:name="com.sabry.log.Reg" />
        <activity android:name="com.sabry.log.NewTest" />
        <activity android:name="com.sabry.log.AboutUs" />
        <activity android:name="com.sabry.log.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>